{
  "name": "acm-config-manager",
  "version": "1.0.0",
  "description": "acm-config-manager",
  "main": "dist/app.js",
  "scripts": {
    "test": "jest  --setupFiles dotenv/config",
    "test:watch": "jest --watch --verbose",
    "dev": "nodemon ./src/server.ts",
    "start": "npm run build; node ./dist/server.js",
    "build": "tsc",
    "schema": "python ./swagger_builder/schema_from_model.py && npm run prettier",
    "swagger": "./src/build_swagger.sh src/swagger/swagger-doc.yaml",
    "prettier": "prettier -l --write src",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf ./dist",
    "check": "npm run prettier && npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/mock-aws-s3": "^2.6.3",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "aws-sdk": "^2.1272.0",
    "axios": "^1.3.4",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.6.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "@jest-mock/express": "^2.0.1",
    "awesome-ajv-errors": "^5.1.0",
    "express": "^4.18.2",
    "jest-mock": "^29.3.1",
    "jsonpointer": "^5.0.1",
    "mock-aws-s3": "^4.0.2",
    "prettier": "^2.8.1",
    "python": "^0.0.4",
    "typeconv": "^1.2.0",
    "winston": "^3.8.2",
    "yaml": "^2.1.3"
  }
}
